def locate_card_binary_jovian(cards, query):
    
    init = 0
    last = len(cards) - 1
    mid = last // 2
    
    
    while init <= last:
        if cards[mid] == query:
            return mid
        elif cards[mid] < query:
            last = mid - 1
            mid = (init + last) // 2
        else:
            init = mid + 1
            mid = (init + last) // 2

    
    return -1





cards = [3, -1, -9, -127]
query = -127

result = locate_card_binary_jovian(cards,query)
print(result)